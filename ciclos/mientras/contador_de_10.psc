Algoritmo contador_de_10
	definir num, contador como entero
	contador <- 0
	escribir "ingrese un digito: (se para en 0)"
	leer num
	mientras num <> 0 Hacer
		escribir "Ingrese digito: (se para en 0)"
		leer num
		si num > 10 Entonces
			contador <- contador + 1
		FinSi
	FinMientras
	Escribir "Cantidad de numeros mayores a 10: ", contador
FinAlgoritmo
